    <xs:element name="ThermalTimeState" msdata:IsDataSet="true" msdata:Locale="en-US">
      <xs:complexType>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element name="VarInfoVariables">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Name" type="xs:string" minOccurs="0" />
                <xs:element name="MinValue" type="xs:double" minOccurs="0" />
                <xs:element name="MaxValue" type="xs:double" minOccurs="0" />
                <xs:element name="DefaultValue" type="xs:double" minOccurs="0" />
                <xs:element name="Units" type="xs:string" minOccurs="0" />
                <xs:element name="Type" type="xs:string" minOccurs="0" />
                <xs:element name="Description" type="xs:string" minOccurs="0" />
                <xs:element name="URL" type="xs:string" minOccurs="0" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="Description">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="DomainClassNameSpace" type="xs:string" minOccurs="0" />
                <xs:element name="DomainTypeName" type="xs:string" minOccurs="0" />
                <xs:element name="DomainTypeURL" type="xs:string" minOccurs="0" />
                <xs:element name="DomainTypeDescription" type="xs:string" minOccurs="0" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:choice>
      </xs:complexType>
    </xs:element>
