    <Description>minimum shoot temperature</Description>
    <URL>http://</URL>
  </VarInfoVariables>
  <VarInfoVariables>
    <Name>maxTshoot</Name>
    <MinValue>-20</MinValue>
    <MaxValue>50</MaxValue>
    <DefaultValue>20</DefaultValue>
    <Units>°C</Units>
    <Type>double</Type>
    <Description>maximum shoot temperature</Description>
    <URL>http://</URL>
  </VarInfoVariables>
  <VarInfoVariables>
    <Name>hourlyShootTemperature</Name>
    <MinValue>-30</MinValue>
    <MaxValue>50</MaxValue>
    <DefaultValue>20</DefaultValue>
    <Units>°C</Units>
    <Type>double[24]</Type>
    <Description>hourly shoot temperature</Description>
    <URL>http://</URL>
  </VarInfoVariables>
  <VarInfoVariables>
    <Name>hourlyAirTemperature</Name>
    <MinValue>-30</MinValue>
    <MaxValue>50</MaxValue>
    <DefaultValue>20</DefaultValue>
