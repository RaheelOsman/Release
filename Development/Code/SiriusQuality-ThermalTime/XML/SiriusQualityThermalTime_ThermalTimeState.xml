  <VarInfoVariables>
    <Name>minTair</Name>
    <MinValue>-30</MinValue>
    <MaxValue>40</MaxValue>
    <DefaultValue>10</DefaultValue>
    <Units>°C</Units>
    <Type>double</Type>
    <Description>minimum air temperature</Description>
    <URL>http://</URL>
  </VarInfoVariables>
  <VarInfoVariables>
    <Name>maxTair</Name>
    <MinValue>-20</MinValue>
    <MaxValue>50</MaxValue>
    <DefaultValue>20</DefaultValue>
    <Units>°C</Units>
    <Type>double</Type>
    <Description>maximum air temperature</Description>
    <URL>http://</URL>
  </VarInfoVariables>
  <VarInfoVariables>
    <Name>minTsoil</Name>
    <MinValue>-30</MinValue>
    <MaxValue>40</MaxValue>
    <DefaultValue>10</DefaultValue>
    <Units>°C</Units>
    <Type>double</Type>
    <Description>minimum soil temperature</Description>
    <URL>http://</URL>
  </VarInfoVariables>
  <VarInfoVariables>
    <Name>maxTsoil</Name>
    <MinValue>-20</MinValue>
    <MaxValue>50</MaxValue>
    <DefaultValue>20</DefaultValue>
    <Units>°C</Units>
    <Type>double</Type>
    <Description>maximum soil temperature</Description>
    <URL>http://</URL>
  </VarInfoVariables>
  <VarInfoVariables>
    <Name>minTshoot</Name>
    <MinValue>-30</MinValue>
    <MaxValue>40</MaxValue>
